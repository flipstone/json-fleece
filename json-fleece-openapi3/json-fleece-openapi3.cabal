source-repository head
  type: git
  location: https://github.com/flipstone/json-fleece-openapi3

flag strict
  description: More strict ghc options used for development and ci, not intended for end-use.
  manual: True
  default: False

library
  exposed-modules:
      Fleece.OpenApi3
  other-modules:
      HaskellCode
      Paths_json_fleece_openapi3
  hs-source-dirs:
      src
  build-depends:
      aeson ==2.0.*
    , base >=4.7 && <5
    , containers ==0.6.*
    , insert-ordered-containers ==0.2.*
    , mtl ==2.2.*
    , openapi3 ==3.2.*
    , text ==1.2.*
    , text-manipulate ==0.3.*
  default-language: Haskell2010
  if flag(strict)
    ghc-options: -Weverything -Werror -Wno-missing-local-signatures -Wno-monomorphism-restriction -Wno-missing-import-lists -Wno-missing-kind-signatures -Wno-prepositive-qualified-module -Wno-implicit-prelude -Wno-safe -Wno-unsafe -Wno-missing-safe-haskell-mode -Wno-missing-deriving-strategies -Wno-all-missed-specialisations -Wno-missed-specialisations
  else
    ghc-options: -Wall

executable fleece-openapi3
  main-is: Main.hs
  other-modules:
      Paths_json_fleece_openapi3
  hs-source-dirs:
      exe
  ghc-options: -rtsopts -threaded
  build-depends:
      base >=4.7 && <5
    , directory ==1.3.*
    , filepath ==1.4.*
    , isocline ==1.0.*
    , json-fleece-openapi3
    , openapi3 ==3.2.*
    , optparse-applicative ==0.17.*
    , text ==1.2.*
    , yaml ==0.11.*
  default-language: Haskell2010
  if flag(strict)
    ghc-options: -Weverything -Werror -Wno-missing-local-signatures -Wno-monomorphism-restriction -Wno-missing-import-lists -Wno-missing-kind-signatures -Wno-prepositive-qualified-module -Wno-implicit-prelude -Wno-safe -Wno-unsafe -Wno-missing-safe-haskell-mode -Wno-missing-deriving-strategies -Wno-all-missed-specialisations -Wno-missed-specialisations
  else
    ghc-options: -Wall

